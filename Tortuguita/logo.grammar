 back = 'BACK';
 bk = 'BK';
 left = 'LEFT';
 lf = 'LF';
 right = 'RIGHT';
 rt = 'RT';
 setpos = 'SETPOS';
 setxy = 'SETXY';
 setx  = 'SETX';
 sety  = 'SETY';
