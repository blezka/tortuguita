Package 
	comp.logo;
Helpers
 letter = ['a'..'z'] | ['A'..'Z'];
 number = ['0'..'9'];
 anychar = [0x0..0xffff];
 cr = 13;
 lf = 10;
 tab = 8;
 underscore = 95;
 eol = (cr | lf | cr lf);
 
Tokens
 word = 'WORD';
 list = 'LIST';
 first = 'FIRST';
 butfirst = 'BUTFIRST';
 bf = 'BF';
 last = 'LAST';
 setitem = 'SETITEM';
 push = 'PUSH';
 pop = 'POP';
 print = 'PRINT';
 pr = 'PR';
 sum = 'SUM';
 difference = 'DIFFERENCE';
 minus = 'MINUS';
 product = 'PRODUCT';
 modulo = 'MODULO';
 int = 'INT';
//// Ale
 round = 'ROUND';
 sqrt = 'SQRT';
 power = 'POWER';
 exp = 'EXP';
 and = 'AND';
 or = 'OR';
 not = 'NOT';
 forward = 'FORWARD';
